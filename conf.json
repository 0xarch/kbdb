{
    "page":{
        "pageroot":"pages",
        "publicroot":"public",
        "pages":{
            "main.ejs":{
                "type":"z-ejs",
                "destination":"root/"
            },
            "server.ejs":{
                "type":"z-ejs",
                "destination":"root/pages/server/"
            },
            "blog.ejs":{
                "type":"z-ejs",
                "destination":"root/pages/blog/"
            },
            "aboutus.ejs":{
                "type":"z-ejs",
                "destination":"root/pages/aboutus/"
            },
            "blog_t.ejs":{
                "type":"t-ejs",
                "template":"blog"
            },
            "wiki_t.ejs":{
                "type":"t-ejs",
                "template":"wiki"
            }
        }
    },
    "toolkit":"mata-toolkit",
    "publicroot":"public",
    "pages":"pages",
    "authors":"conf.d/authors.json",
    "types":{
        "blog":{
            "conf":"conf.d/blogs.json",
            "source":"public/src/blog/",
            "type":"blog",
            "template":"blog",
            "enabled":true
        },
        "wiki":{
            "conf":"conf.d/wikis.json",
            "source":"public/src/wiki/",
            "type":"wiki",
            "template":"wiki",
            "enabled":false
        }
    },
    "palette":{
        "gencolor":["LIME"],
        "primary":"LIME500",
        "secondary":"LIME"
    },
    "site":{
        "name":"0xDevel",
        "short":"Web Studio",
        "cdn_repo_rootdir":"https://cdn.jsdelivr.net/gh/0xarch/0xarch.github.io/"
    }
}